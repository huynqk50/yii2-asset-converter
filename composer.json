{
    "name": "huynqk50/yii2-asset-converter",
    "description": "Less, Sass, Scss and Phamlp converter for Yii2. No system requires. yii2-composer support, Less autoupdate, customizing of output directory. Updated to work with php 8.2. Forked from nizsheanez/yii2-asset-converter",
    "type": "yii2-extension",
    "keywords": ["yii", "yii2", "asset", "converter", "less", "sass", "scss", "compass", "phamlp"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "huynq",
            "email": "huynqk50@gmail.com",
            "role": "Author"
        }
    ],
    "require" : {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-composer": "2.*",
        "wikimedia/less.php": "3.0.0.*",
        "richthegeek/phpsass": "1.*",
        "scssphp/scssphp": "1.*"
    },
    "autoload": {
        "psr-0": { "nizsheanez\\assetConverter\\": "" }
    },
    "target-dir": "huynq/assetConverter"
}
